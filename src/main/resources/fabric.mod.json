{
  "schemaVersion": 1,
  "id": "log-ip-remover",
  "version": "${version}",
  "name": "Log IP Remover",
  "description": "A mod which removes player's public IP addresses from server logs when they join a server.",
  "authors": [
    "YoArcane"
  ],
  "contact": {
    "homepage": "https://github.com/YoArcane/LogIPRemover",
    "sources": "https://github.com/YoArcane/LogIPRemover",
    "issues": "https://github.com/YoArcane/LogIPRemover/issues"
  },
  "license": "LGPL-3.0",
  "icon": "assets/log-ip-remover/icon.png",
  "environment": "server",
  "entrypoints": {
    "server": [
      "me.yoarcane.logipremover.LogIPRemoverMod"
    ]
  },
  "mixins": [
    "log-ip-remover.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
